<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:autoTransition="none"
        motion:motionInterpolator="cubic(0.39,1,0.1,0.06)">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnClick motion:targetId="@id/fab" />
        <OnClick motion:targetId="@+id/add_schedule_Btn" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="127dp"
            android:id="@+id/add_schedule_Btn"
            motion:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
            android:visibility="invisible"
            android:layout_marginBottom="0dp"
            android:scaleX="0.1" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/add_schedule_Btn"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.927" />
    </ConstraintSet>
</MotionScene>