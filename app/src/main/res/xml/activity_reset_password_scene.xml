<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:motionInterpolator="cubic-bezier(.4,.84,0,1)">
        <KeyFrameSet>
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintVertical_bias="0.499"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="234dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView5"
            android:scaleX="1.3"
            android:scaleY="1.3"
            android:visibility="invisible"
            android:translationX="190dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintHorizontal_bias="0.494"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/reset_password_img"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="191dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintHorizontal_bias="0.496"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/enter_your_"
            android:layout_marginTop="0dp"
            motion:layout_constraintTop_toBottomOf="@+id/reset_passw" />
        <Constraint
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteY="290dp"
            android:translationX="300dp"
            android:translationY="-60dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="188dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.495"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/backBtn2"
            android:id="@+id/resetEmail_input" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            motion:layout_constraintVertical_bias="0.499"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView5"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:id="@+id/reset_password_img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            android:translationX="-50dp"
            motion:layout_editor_absoluteX="42dp"
            motion:layout_constraintBottom_toTopOf="@+id/resetEmail_input"
            android:layout_marginBottom="32dp"
            motion:layout_constraintTop_toBottomOf="@+id/reset_passw"
            motion:layout_constraintVertical_bias="0.759" />
        <Constraint
            android:id="@+id/reset_password_img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            motion:layout_editor_absoluteX="42dp"
            motion:layout_constraintBottom_toTopOf="@+id/resetEmail_input"
            android:layout_marginBottom="32dp"
            motion:layout_constraintTop_toBottomOf="@+id/reset_passw"
            motion:layout_constraintVertical_bias="0.759" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintHorizontal_bias="0.482"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/enter_your_"
            android:layout_marginTop="4dp"
            motion:layout_constraintTop_toBottomOf="@+id/reset_passw" />
        <Constraint
            motion:layout_editor_absoluteY="254dp"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.498"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView2"
            android:translationY="-60dp" />
    </ConstraintSet>
</MotionScene>